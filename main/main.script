local IPTools = require('iptools.core'):new()

function init(self)

	-- IPTools.debug = true
	
	IPTools:get_public_ip(function(ip_address)
		print(ip_address)
		IPTools:get_location(ip_address, function(info)
			for k,v in pairs(info) do 
				print(k .. " = " .. v)
			end
		end)
	end)

	-- IPTools:print_local_ip()
end
